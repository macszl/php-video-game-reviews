<div class="gallery-profile">
    <h3>Reviews</h3>
    {% for item in items %}
    <div class="gallery-item">
        <a
        class="gallery-picture-link"
        href="{{item.imagelink}}"
        ></a>
        <div class="item-info">
            <p class="name"> {{ item.title }}</p>
            <p class="date">{{item.username}} REVIEWED THIS AT {{item.date}}</p>
            <p class="comment-post">{{item.description}}</p>
        </div>
    </div>
    {% endfor %}
</div>